<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>DataControllerServlet</servlet-name>
  	<display-name>DataControllerServlet</display-name>
  	<description></description>
  	<servlet-class>hw4Kchung2.DataControllerServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SearchServlet</servlet-name>
  	<display-name>SearchServlet</display-name>
  	<description></description>
  	<servlet-class>hw4Kchung2.SearchServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Productservlet</servlet-name>
  	<display-name>Productservlet</display-name>
  	<description></description>
  	<servlet-class>hw4Kchung2.Productservlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>proservlet</servlet-name>
  	<display-name>proservlet</display-name>
  	<description></description>
  	<servlet-class>hw4Kchung2.proservlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>checkout</servlet-name>
  	<display-name>checkout</display-name>
  	<description></description>
  	<servlet-class>hw4Kchung2.checkout</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
  	<servlet-name>DataControllerServlet</servlet-name>
  	<url-pattern>/cust_login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SearchServlet</servlet-name>
  	<url-pattern>/search</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Productservlet</servlet-name>
  	<url-pattern>/Product</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>proservlet</servlet-name>
  	<url-pattern>/shoppingcart</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>checkout</servlet-name>
  	<url-pattern>/Checkout</url-pattern>
  </servlet-mapping>
 
</web-app>
